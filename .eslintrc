{
  "extends": [
    "google"
  ],
  "rules": {
    "comma-dangle": 0,
    "require-jsdoc": ["error", {
    "require": {
      "FunctionDeclaration": false,
      "MethodDefinition": false,
      "ClassDeclaration": false,
      "ArrowFunctionExpression": false,
      "FunctionExpression": false
    }
  }]
  },
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
